<div class="col-md-6 col-md-offset-3">
  <p id="notice"><%= notice %></p>
  <% @todo_lists.each do |todo_list| %>
    <div class="well">
      <h1><%= todo_list.title %></h1>
      <h5><%= todo_list.description %></h5>
      <hr/>
      <%= link_to 'New Todo list', new_todo_list_path %>
    </div>
  <% end %>
</div>
